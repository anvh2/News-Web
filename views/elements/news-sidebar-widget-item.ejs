<%if((isAuthenticated && news.type === 'premium') || news.type === 'normal') {%>
<div class="single-blog-post d-flex">
    <div class="post-thumbnail">
        <img src="<%-news.image%>" alt="">
    </div>
    <div class="post-content">
        <a href="/news/<%-news.nid%>" class="post-title"><p style="font-size: 13px"><%-news.title%></p></a>
        <div class="post-meta d-flex justify-content-between">
            <a href="/news/<%-news.nid%>"><i class="fa fa-eye" aria-hidden="true"></i> <%-news.views%></a>
            <a href="/news/<%-news.nid%>"><i class="fa fa-comments" aria-hidden="true"> <%-news.total_cmt%></i></a>
            <!--<a href=<%-"news/"+news.nid%>><i class="fa fa-thumbs-up" aria-hidden="true"></i> 84</a>-->
        </div>
    </div>
</div>
<%}%>